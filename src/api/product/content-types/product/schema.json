{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Price": {
      "type": "biginteger",
      "required": true
    },
    "Main_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "Sub_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "Product_details": {
      "type": "richtext"
    },
    "Product_details_title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "Name"
    },
    "filter": {
      "type": "enumeration",
      "enum": [
        "than-kinh",
        "xuong-khop",
        "tieu-hoa",
        "tim-mach",
        "tram-cam",
        "than-tiet-nieu",
        "noi-tiet"
      ]
    }
  }
}
